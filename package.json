{
  "name": "mop",
  "version": "0.12.5",
  "title": "Montage Optimizer",
  "description": "Optimizes Montage applications for production by minifying, to reduce file size, and creating bundles, to reduce the number of requests.",
  "keywords": ["montage", "optimization", "production"],
  "main": "optimize.js",
  "scripts": {
    "test": "./node_modules/.bin/jasmine-node spec"
  },
  "dependencies": {
    "montage": "~0.12.2",
    "mr": "~0.12.6",
    "collections": "~0.1.0",
    "q": "~0.8.11",
    "q-io": "~1.5",
    "url2": "~0.0.0",
    "semver": "~1.1",
    "uglify-js": "~1",
    "optimist": "~0.2.8",
    "jsdom": "~0.2.15",
    "csso": "~1.3.7",
    "html-minifier": "~0.4.5"
  },
  "devDependencies": {
    "jasmine-node": "~1.5.0",
    "sandboxed-module": "0.1.4"
  },
  "exclude": [
    "builds"
  ],
  "bin": {
    "mop": "optimize.js"
  },
  "preferGlobal": true
}
